{
  "name": "cnblogs-img",
  "displayName": "cnblogs-img",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cnblogs-img.login_test",
        "title": "Login Test"
      },
      {
        "command": "cnblogs-img.upload-from-context",
        "title": "upload cnblogs"
      },
      {
        "command": "cnblogs-img.just-for-test",
        "title": "Just For Test"
      },
      {
        "command": "cnblogs-img.replace-markdown",
        "title": "Replace Markdown"
      }
    ],
    "configuration": {
      "title": "cnblogs-img",
      "properties": {
        "cnblogs.cookie.AspNetCore": {
          "type": "string",
          "default": "",
          "description": "cnblogs的.Cnblogs.AspNetCore.Cookies字段"
        },
        "cnblogs.cookie.CNBlogsCookie": {
          "type": "string",
          "default": "",
          "description": "cnblogs的.CNBlogsCookie字段"
        }
      }
    },
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .jpg||  resourceExtname == .gif || resourceExtname == .png",
          "command": "cnblogs-img.upload-from-context",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.52.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.15.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "@types/superagent": "^4.1.10",
    "clipboardy": "^2.3.0"
  }
}
